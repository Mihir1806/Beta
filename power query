

= Text.Combine(
    List.Select(
        Text.Split(Text.FromBinary([Content]), "#(lf)"),
        each Text.Contains(_, "error", Comparer.OrdinalIgnoreCase)
    ),
    "#(lf)"
)





MergedTable = 
ADDCOLUMNS(
    Mail,
    "DateAccessed", RELATED(FLEXEL_LOG_IN[Date accessed]),
    "DateCreated", RELATED(FLEXEL_LOG_IN[Date created]),
    "ExecutionTimeIN", RELATED(FLEXEL_LOG_IN[Execution Time IN])
)