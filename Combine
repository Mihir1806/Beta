IF StartsWith([YourColumn], "ABC") THEN 
    RIGHT([YourColumn], Length([YourColumn]) - 3) 
ELSE 
    [YourColumn] 
ENDIF





IF StartsWith([YourColumn], "ABC") THEN 
    [YourColumn] = RIGHT([YourColumn], Length([YourColumn]) - 3)
ENDIF;

IF EndsWith([YourColumn], ".yxmd") THEN 
    [YourColumn] = ""
ENDIF




IF StartsWith([YourColumn], "ABC") THEN 
    IIF(EndsWith(RIGHT([YourColumn], Length([YourColumn]) - 3), ".yxmd"), "", RIGHT([YourColumn], Length([YourColumn]) - 3)) 
ELSE 
    IIF(EndsWith([YourColumn], ".yxmd"), "", [YourColumn]) 
ENDIF