IF StartsWith([YourColumn], "ABC") THEN 
    RIGHT([YourColumn], Length([YourColumn]) - 3) 
ELSE 
    [YourColumn] 
ENDIF





IF StartsWith([YourColumn], "ABC") THEN 
    IIF(EndsWith([YourColumn], ".yxmd"), 
        LEFT(RIGHT([YourColumn], Length([YourColumn]) - 3), Length([YourColumn]) - 3 - 5), 
        RIGHT([YourColumn], Length([YourColumn]) - 3)
    )
ELSE 
    IIF(EndsWith([YourColumn], ".yxmd"), 
        LEFT([YourColumn], Length([YourColumn]) - 5), 
        [YourColumn]
    )
ENDIF