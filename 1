=LET(
 d, DATEVALUE(SUBSTITUTE(STATEMENT!E2,"/","-")),
 r, AGGREGATE(15,6,ROW(A!A1:AA25)/(DATEVALUE(SUBSTITUTE(A!A1:AA25,"-","/"))=d),1),
 c, AGGREGATE(15,6,COLUMN(A!A1:AA25)/(DATEVALUE(SUBSTITUTE(A!A1:AA25,"-","/"))=d),1),
 INDEX(A!A:AA,
   r + MATCH(TRUE, INDEX(INDEX(A!A:AA,,c)<>"" , r+1):INDEX(INDEX(A!A:AA,,c)<>"" , r+5), 0),
   c
 )
)