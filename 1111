let
    // Load the webpage
    Source = Web.Page(Web.Contents("https://fedai.org.in/RevaluationRates.aspx?Cid=1&SCid=0")),
    Data = Source{0}[Data],

    // Rename columns for clarity
    Renamed = Table.RenameColumns(Data, {{"Column1", "Date & Time"}, {"Column2", "Revaluation Rates"}}),

    // Extract actual hyperlink from anchor tag in "Revaluation Rates"
    AddLinkColumn = Table.AddColumn(Renamed, "Excel URL", each try Text.BetweenDelimiters([Revaluation Rates], "href=""", """") otherwise null),

    // Fix relative URLs to full FEDAI links
    FullURLs = Table.TransformColumns(AddLinkColumn, {
        "Excel URL", each if _ <> null and Text.StartsWith(_, "/") then "https://fedai.org.in" & _ else _
    })
in
    FullURLs