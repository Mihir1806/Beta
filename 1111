let
    // Get the full raw HTML page
    RawHTML = Text.FromBinary(Web.Contents("https://fedai.org.in/RevaluationRates.aspx?Cid=1&SCid=0")),

    // Split by <tr> tags to get each table row
    Rows = List.Skip(Text.Split(RawHTML, "<tr"), 1),

    // Extract date and href from each row
    Extracted = List.Transform(Rows, each [
        RowText = Text.BeforeDelimiter(_, "</tr>"),

        // Extract date column (first <td>)
        DateRaw = try Text.BetweenDelimiters(RowText, "<td>", "</td>") otherwise null,

        // Extract href (second column with <a href>)
        LinkRaw = try Text.BetweenDelimiters(RowText, "href=""", """") otherwise null,

        // Convert relative to full URL
        FullURL = if LinkRaw <> null and Text.StartsWith(LinkRaw, "/") then "https://fedai.org.in" & LinkRaw else LinkRaw,

        Output = [Date = DateRaw, ExcelURL = FullURL]
    ][Output]),

    // Convert to table
    ToTable = Table.FromList(Extracted, Record.ToTable, {"Row"}),

    // Expand the inner records
    Expanded = Table.ExpandRecordColumn(ToTable, "Row", {"Date", "ExcelURL"})
in
    Expanded


let
    Source = Text.FromBinary(Web.Contents("https://fedai.org.in/RevaluationRates.aspx?Cid=1&SCid=0")),
    Rows = List.Skip(Text.Split(Source, "<tr"), 1),
    
    Parsed = List.Transform(Rows, each
        let
            row = Text.BeforeDelimiter(_, "</tr>"),
            dateTime = try Text.BetweenDelimiters(row, "<td>", "</td>") otherwise null,
            label = try Text.BetweenDelimiters(row, ">", "</a>") otherwise null,
            href = try Text.BetweenDelimiters(row, "href=""", """") otherwise null,
            fullLink = if href <> null then "https://fedai.org.in" & href else null
        in
            [#"Date & Time" = dateTime, Label = label, Hyperlink = fullLink]
    ),
    
    Output = Table.FromRecords(Parsed)
in
    Output